<% @entries.each do |e| %>
  <% if @myUserId != e.user.id %>
    <h2><%= e.user.name %>さんとのチャット</h2>
  <% end %>
<% end %>

<% @messages.each do |m| %>
  <p><%= m.user.name %></p>
  <p><%= m.message %></p>
<% end %>

<%= form_with model: @message do |f| %>
  <%= f.text_field :message %>
  <%= f.hidden_field :room_id, value: @room.id %>
  <%= f.submit "送信" %>
<% end %>